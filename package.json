{
  "name": "domieuro4",
  "version": "1.1.0",
  "description": "Production-ready B2B SaaS starter template with React Router, WorkOS authentication, Convex database, and Stripe billing",
  "private": true,
  "type": "module",
  "keywords": [
    "react-router",
    "workos",
    "convex",
    "stripe",
    "billing",
    "saas",
    "b2b",
    "starter-template",
    "authentication",
    "multi-tenant",
    "subscriptions",
    "rbac",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
